 <header class="home-header">
        <h1><a href="/">Quiz Hub</a></h1>
        <nav>
            <ul>
                <li>
                    <a href="/createQuiz">Create</a>
                </li>
                <li>
                    <a href="/scores">Scores</a>
                </li>
                <li>
                    <div class="dropdown dropbtn">
                    <a href="/highscores">Categories</a>
                    <i class="fa fa-caret-down"></i>
                    <div class="dropdown-content">
                        {{#each categories as |category|}}
                        <a href="/category/{{category.id}}">{{category.category_name}}</a>
                        {{/each}}
                    </div>
                    </div>
                </li>
                <li>
                    <a href="#" id="logout">Logout</a>
                </li>
            </ul>
        </nav>
    </header>
    <main>

    <h2 class="welcome"> &#x26AC; Welcome to our Hub of Quizzes &#x26AC;</h2> 
    <p class="welcomesm"> 
    Links are above &#8593; and a sampling of user-made quizzes are below &#8595; </p>

    <div class="homepage-container">
        {{#each quizzes as |quiz|}}
        <div><a href="/quiz/{{quiz.id}}">{{quiz.quiz_name}}</a></div>
        {{/each}}
    </div>
        
</main>

<script src="/js/logout.js"></script>