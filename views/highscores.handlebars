 <header class="home-header">
        <h1><a href="/">Quiz Hub</a></h1>
        <nav>
            <ul>
                <li>
                    <a href="/createQuiz">Create Quiz</a>
                </li>
                <li>
                    <a href="/scores">My Scores</a>
                </li>
                <li>
                    <div class="dropdown dropbtn">
                    <a href="/highscores">Categories</a>
                    <i class="fa fa-caret-down"></i>
                    <div class="dropdown-content">
                        {{#each categories as |category|}}
                        <a href="/category/{{category.id}}">{{category.category_name}}</a>
                        {{/each}}
                    </div>
                    </div>
                </li>
                <li>
                    <a href="#" id="logout">Log Out</a>
                </li>
            </ul>
        </nav>
    </header>
    <main>

    <h2> High Scores &#8595; </h2>

        <div class="homepage-container">
    {{#each categories as |category|}}
        <div class="scorebox">{{category_name}}
        <ol>
    {{#each ../scores as |score|}}
            <li>{{quiz.quiz_name}} {{total_score}}</li>
    {{/each}}
        </ol>
        </div>
    {{/each}}
        </div>
    
       
</main>

<script src="/js/logout.js"></script>